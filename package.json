{
  "name": "olleego",
  "version": "0.0.1",
  "description": "공모전",
  "author": {
    "name": "Lee SungKyu",
    "url": "http://github.com/sinrim10"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index -w 'src/**/*' --ext 'js json'",
    "debug": "nodemon index --debug -w 'src/**/*' --ext 'js json'",
    "lint": "jshint src/**/*.js",
    "audit": "nsp audit-shrinkwrap && nsp audit-package",
    "missing": "npm ls --depth 1",
    "outdated": "npm outdated --depth 0",
    "prepush": "npm shrinkwrap && npm test",
    "postmerge": "npm install",
    "pretest": "npm run lint"
  },
  "pre-commit": [
    "lint",
    "audit",
    "missing",
    "outdated"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/martinmicunda/ionic-photo-gallery"
  },
  "bugs": {
    "url": "https://github.com/martinmicunda/ionic-photo-gallery/issues"
  },
  "files": [
    "src",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "mm",
    "ionic",
    "node",
    "express",
    "mobile"
  ],
  "dependencies": {
    "apidoc": "^0.16.1",
    "async": "^2.0.0-rc.6",
    "aws-sdk": "^2.4.9",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.1.0",
    "bluebird": "^3.4.0",
    "body-parser": "^1.12.2",
    "colors": "^1.0.3",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "connect-redis": "^3.0.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-alpha.1",
    "cors": "^2.5.3",
    "ejs": "^2.4.2",
    "express": "^4.12.3",
    "express-flash": "0.0.2",
    "express-session": "^1.13.0",
    "express-validator": "^2.20.8",
    "glob": "^5.0.3",
    "gulp": "^3.9.1",
    "gulp-apidoc": "^0.2.4",
    "helmet": "^0.7.0",
    "https": "^1.0.0",
    "iconv-lite": "^0.4.13",
    "jade": "^1.11.0",
    "jsdom": "^9.4.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^3.5.0",
    "method-override": "^2.3.2",
    "mm-node-logger": "^0.0.*",
    "mobile-detect": "^1.3.2",
