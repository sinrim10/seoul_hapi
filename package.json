    "multer-s3": "^2.3.2",
    "node-gcm": "^0.14.3",
    "nodemailer": "^2.4.2",
    "nodemailer-smtp-transport": "^2.5.0",
    "passport": "^0.2.1",
    "passport-facebook": "^2.1.1",
    "passport-facebook-token": "^3.3.0",
    "passport-kakao": "0.0.4",
    "passport-local": "^1.0.0",
    "path": "^0.11.14",
    "pryjs": "^1.0.2",
    "redis": "^2.6.2",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "gulp-apidoc-swagger": "^0.2.2",
